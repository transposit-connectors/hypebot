{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "sarah/merriam_webster:latest",
        "alias": "merriam_webster_dict"
      }, {
        "name": "sarah/emojidex:latest",
        "alias": "emojidex"
      }, {
        "name": "sarah/merriam_webster:latest",
        "alias": "merriam_webster_thes"
      }, {
        "name": "transposit/apify_crawler:v1.*.*",
        "alias": "apify_crawler"
      }],
      "operations": [{
        "id": "hello_world",
        "description": "Returns \"Hello, world\" in different languages",
        "function": {
          "js": "hello_world.js"
        },
        "access": {
          "requireUser": true
        }
      }, {
        "id": "def_and_syn",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "\nSELECT meta.id, meta.uuid, hwi.hw, fl, syns, shortdef FROM merriam_webster_dict.collegiate_dictionary_search\n  WHERE word=@word LIMIT 3 /* if it's not in the first 3, it'll probably never happen*/",
          "cacheExpirySec": 86400
        }
      }, {
        "id": "extract_syn",
        "function": {
          "parameters": [{
            "name": "dict_results",
            "type": "ARRAY"
          }],
          "js": "extract_syn.js"
        }
      }, {
        "id": "word_search",
        "function": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "js": "word_search.js",
          "cacheExpirySec": 84600
        }
      }, {
        "id": "get_emoji",
        "tql": {
          "parameters": [{
            "name": "name",
            "type": "STRING"
          }],
          "tql": "SELECT code, moji FROM emojidex.get_emoji\n  WHERE name=@name"
        }
      }, {
        "id": "collegiate_thesaurus_search",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "SELECT meta.syns FROM merriam_webster_thes.collegiate_thesaurus_search\n  WHERE word=@word expand by meta.syns LIMIT 3"
        }
      }, {
        "id": "search_emoji",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "SELECT emoji FROM emojidex.search_emoji\n  WHERE code_cont=@word expand by emoji"
        }
      }, {
        "id": "sentence_seeker",
        "function": {
          "parameters": [{
            "name": "sentence",
            "type": "STRING"
          }],
          "js": "sentence_seeker.js"
        }
      }, {
        "id": "filtered_def_and_syn",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM this.def_and_syn\n  WHERE word=@word AND fl != \"indefinite article\" AND fl != \"definite article\" AND fl != \"preposition\" AND fl != \"conjunction\" AND fl != \"abbreviation\" AND fl != \"prefix\" AND fl != \"noun suffix\" AND fl != \"adjective suffix\" AND fl !=\"combining form\""
        }
      }, {
        "id": "webhook",
        "function": {
          "parameters": [{
            "name": "http_event",
            "default": {
              "text": "{\"http_method\": \"GET\", \"query_parameters\": {}, \"headers\": {}, \"body\": \"\"}"
            },
            "type": "OBJECT"
          }],
          "js": "webhook.js"
        },
        "access": {
          "requireApiKey": true,
          "webhook": true
        }
      }, {
        "id": "execute_crawler",
        "tql": {
          "tql": "SELECT * FROM apify_crawler.execute_crawler\n  WHERE user_id='sZs7Ng4R5xrAR756J'\n  AND crawler_id='EoP97LXxAykJrQPZG'\n  AND wait='120'"
        }
      }, {
        "id": "get_crawler_last_execution_results",
        "tql": {
          "tql": "SELECT pageFunctionResult.releaseNotes FROM apify_crawler.get_crawler_last_execution_results\n  WHERE user_id='sZs7Ng4R5xrAR756J'\n  AND crawler_id='EoP97LXxAykJrQPZG'"
        }
      }],
      "dataConnections": [{
        "ref": "emojidex",
        "overrides": {
          "swagger": {
            "executionLimits": {
              "maxApiCalls": 5000
            }
          }
        }
      }],
      "javascriptAPIVersion": "3"
    }
  }
}