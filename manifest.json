{
  "v2": {
    "transposit": {
      "dependencies": [{
        "name": "sarah/merriam_webster:latest",
        "alias": "merriam_webster_dict"
      }, {
        "name": "sarah/emojidex:latest",
        "alias": "emojidex"
      }, {
        "name": "sarah/merriam_webster:latest",
        "alias": "merriam_webster_thes"
      }],
      "operations": [{
        "id": "hello_world",
        "description": "Returns \"Hello, world\" in different languages",
        "function": {
          "js": "hello_world.js"
        },
        "access": {
          "requireUser": true
        }
      }, {
        "id": "def_and_syn",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "\nSELECT meta.id, meta.uuid, hwi.hw, fl, syns, shortdef FROM merriam_webster_dict.collegiate_dictionary_search\n  WHERE word=@word LIMIT 3 /* if it's not in the first 3, it'll probably never happen*/",
          "cacheExpirySec": 86400
        }
      }, {
        "id": "extract_syn",
        "function": {
          "parameters": [{
            "name": "dict_results",
            "type": "ARRAY"
          }],
          "js": "extract_syn.js"
        }
      }, {
        "id": "word_search",
        "function": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "js": "word_search.js",
          "cacheExpirySec": 846000
        }
      }, {
        "id": "get_emoji",
        "tql": {
          "parameters": [{
            "name": "name",
            "type": "STRING"
          }],
          "tql": "SELECT moji FROM emojidex.get_emoji\n  WHERE name=@name"
        }
      }, {
        "id": "collegiate_thesaurus_search",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "SELECT meta.syns FROM merriam_webster_thes.collegiate_thesaurus_search\n  WHERE word=@word expand by meta.syns LIMIT 3"
        }
      }, {
        "id": "search_emoji",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "SELECT emoji FROM emojidex.search_emoji\n  WHERE code_cont=@word expand by emoji"
        }
      }, {
        "id": "sentence_seeker",
        "function": {
          "parameters": [{
            "name": "sentence",
            "type": "STRING"
          }],
          "js": "sentence_seeker.js"
        }
      }, {
        "id": "filtered_def_and_syn",
        "tql": {
          "parameters": [{
            "name": "word",
            "type": "STRING"
          }],
          "tql": "SELECT * FROM this.def_and_syn\n  WHERE word=@word AND fl != \"indefinite article\" AND fl != \"definite article\" AND fl != \"preposition\" AND fl != \"conjunction\" AND fl != \"abbreviation\" AND fl != \"prefix\" AND fl != \"noun suffix\" AND fl != \"adjective suffix\" AND fl !=\"combining form\""
        }
      }],
      "javascriptAPIVersion": "3"
    }
  }
}